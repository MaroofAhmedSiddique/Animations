<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.animations.maroof">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.OneShotSimpleActivity" />
        <activity android:name=".activities.OneShotAdvancedActivity" />
        <activity android:name=".activities.EmitterSimpleActivity" />
        <activity android:name=".activities.EmitterBackgroundActivity" />
        <activity android:name=".activities.EmitterIntermediateActivity" />
        <activity android:name=".activities.EmitterTimeLimitedActivity" />
        <activity android:name=".activities.EmitterWithGravityActivity" />
        <activity android:name=".activities.FollowCursorActivity" />
        <activity android:name=".activities.AnimatedParticlesActivity" />
        <activity android:name=".activities.FireworksActivity" />
        <activity android:name=".activities.ConfettiActivity" />

    </application>

</manifest>